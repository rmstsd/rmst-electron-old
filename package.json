{
  "name": "rmst-tools",
  "description": "rmst-工具",
  "version": "1.0.2",
  "private": false,
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "electron-dev": "node scripts/watch.mjs",
    "electron-build": "npm run clean-dist && cross-env MODE=production npm run build && electron-builder -c .electron-builder.config.js",
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "postinstall": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/update-electron-vendors.mjs",
    "clean-dist": "shx rm -rf dist"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "18.14.1",
    "@types/path-browserify": "^1.0.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "cross-env": "7.0.3",
    "electron": "23.1.1",
    "electron-builder": "23.6.0",
    "electron-winstaller": "^5.1.0",
    "happy-dom": "8.7.2",
    "nano-staged": "0.8.0",
    "playwright": "1.31.1",
    "shx": "^0.3.4",
    "simple-git-hooks": "2.8.1",
    "typescript": "4.9.5",
    "unplugin-auto-expose": "0.0.4",
    "vite": "4.1.4"
  },
  "dependencies": {
    "@arco-design/web-react": "^2.40.2",
    "@octokit/core": "^4.2.0",
    "@types/react-highlight-words": "^0.16.4",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "cross-spawn": "^7.0.3",
    "crypto-js": "^4.1.1",
    "electron-store": "^8.1.0",
    "electron-updater": "5.3.0",
    "fs-extra": "^11.1.0",
    "less": "^4.1.3",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-highlight-words": "^0.20.0",
    "react-router-dom": "^6.8.1",
    "uiohook-napi": "^1.5.0"
  },
